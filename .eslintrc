{
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": [
      "error",
      4
    ],
    "react/jsx-indent": [
      2,
      4
    ],
    "import/prefer-default-export": "off",
    "react/prop-types": "off",
    "react/jsx-wrap-multilines": "off",
    "import/extensions": "off",
    "react/jsx-closing-tag-location": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}